<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>Meow.Domain</AssemblyName>
    <RootNamespace>Meow.Domain</RootNamespace>
    <Version>1.0.0.2</Version>
    <Authors>程序喵</Authors>
    <Product>Meow.Domain</Product>
    <Description>Meow应用框架 - 领域层类库。
以 何镇汐（https://github.com/dotnetcore/Util）应用框架为蓝本。
暂供个人学习和使用。</Description>
    <Copyright>程序喵</Copyright>
    <PackageProjectUrl>https://github.com/program-meow/meow</PackageProjectUrl>
    <RepositoryUrl>https://github.com/program-meow/meow</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>meow;dotnetcore</PackageTags>
    <PackageReleaseNotes>为个人学习，若喜欢可随意使用。</PackageReleaseNotes>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>..\..\..\output\src\release\</OutputPath>
    <DocumentationFile>..\..\..\output\src\release\Meow.Domain.xml</DocumentationFile>
    <GenerateSerializationAssemblies>Off</GenerateSerializationAssemblies>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>..\..\..\output\src\release\</OutputPath>
    <DocumentationFile>..\..\..\output\src\release\Meow.Domain.xml</DocumentationFile>
    <GenerateSerializationAssemblies>Off</GenerateSerializationAssemblies>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\01-core\Meow.Auth\02-Meow.Auth.csproj" />
    <ProjectReference Include="..\..\01-core\Meow\01-Meow.Core.csproj" />
  </ItemGroup>

</Project>
